{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "vite preview --host 0.0.0.0 --port 5173",
    "test:unit": "vitest",
    "test:unit:verbose": "vitest --reporter=verbose",
    "test:unit:watch": "vitest --watch",
    "test:unit:watch:verbose": "vitest --watch --reporter=verbose",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest --coverage",
    "test:unit:run": "vitest run",
    "test:unit:run:verbose": "vitest run --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:verbose": "playwright test --reporter=verbose",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test": "npm run test:unit && npm run test:e2e",
    "test:verbose": "npm run test:unit:verbose && npm run test:e2e:verbose",
    "test:watch": "npm run test:unit:watch",
    "test:mutation": "stryker run",
    "test:mutation:ci": "stryker run --concurrency 1 --timeoutMS 180000",
    "test:mutation:report": "stryker run --reporter html",
    "test:mutation:taskmodal": "stryker run --mutate src/components/kanban/TaskModal.vue",




    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/",
    "dev:mobile": "vite --host 0.0.0.0 --port 5173",
    "build:mobile": "vite build && npx cap sync",
    "mobile:dev": "vite --host 0.0.0.0 --port 5173",
    "mobile:build": "vite build && npx cap sync",
    "mobile:serve": "vite preview --host 0.0.0.0 --port 5173",
    "android:open": "npx cap open android",
    "android:run": "npx cap run android --target=<device-id>",
    "android:build": "npm run mobile:build && npx cap build android",
    "android:sync": "npx cap sync android",
    "android:list": "npx cap run android --list",
    "ios:open": "npx cap open ios",
    "ios:run": "npx cap run ios --target=<device-id>",
    "ios:build": "npm run mobile:build && npx cap build ios",
    "ios:sync": "npx cap sync ios",
    "mobile:clean": "rm -rf android ios dist node_modules/.cache",
    "mobile:reinstall": "npm run mobile:clean && npm install && npx cap sync"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/app": "^7.1.0",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/network": "^7.0.2",
    "@heroicons/vue": "^2.2.0",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "lightningcss": "^1.30.2",
    "lodash": "^4.17.21",
    "pinia": "^3.0.3",
    "rollup": "^4.53.2",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/html-reporter": "^3.1.0",
    "@stryker-mutator/javascript-mutator": "^4.0.0",
    "@stryker-mutator/vitest-runner": "^9.4.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.23",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.37.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.0",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}
