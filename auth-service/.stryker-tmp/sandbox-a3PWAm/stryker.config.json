{
  "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
  "packageManager": "npm",
  "reporters": ["html", "clear-text", "progress"],
  "testRunner": "jest",
  "coverageAnalysis": "perTest",
  "mutate": [
    "src/auth/**/*.ts",
    "!src/auth/**/*.spec.ts",
    "!src/auth/**/*.test.ts"
  ],
  "mutator": {
    "excludedMutations": [
      "StringLiteral",
      "TemplateLiteral"
    ]
  },
  "jest": {
    "configFile": "./jest.config.js"
  },
  "checkers": ["typescript"],
  "tsconfigFile": "./tsconfig.json",
  "concurrency": 2,
  "timeoutMS": 60000,
  "tempDirName": ".stryker-tmp",
  "thresholds": {
    "high": 85,
    "low": 75,
    "break": 65
  }
}